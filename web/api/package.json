{
  "name": "api",
  "version": "1.0.0",
  "description": "a server-side application that serves as api for reconna.",
  "main": "src/app.js",
  "scripts": {
    "prod": "node ./src/app.js",
    "dev": "nodemon ./src/app.js",
    "test": "mocha ./src/**/*.test.js",
    "lint": "eslint . --ext .js",
    "kill": "killall node",
    "dock-u": "docker-compose up -d",
    "dock-l": "docker-compose logs",
    "dock-d": "docker-compose down",
    "dock-rm": "docker system prune",
    "dock-rmv": "docker system prune --volumes"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonathanferreras/reconna/tree/master/web/api"
  },
  "author": "Jonathan Ferreras",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "email-existence": "^0.1.6",
    "express": "^4.17.1",
    "mongoose": "^5.6.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1"
  }
}
